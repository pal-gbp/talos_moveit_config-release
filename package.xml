<package format="3">
  <name>talos_moveit_config</name>
  <version>2.0.1</version>
  <description>
     An automatically generated package with all the configuration and launch files for using the talos with the MoveIt! Motion Planning Framework
  </description>
  <author email="luca.marchionni@pal-robotics.com">Luca Marchionni</author>
  <maintainer email="luca.marchionni@pal-robotics.com">Luca Marchionni</maintainer>
  <maintainer email="ilenia.perrella@pal-robotics.com">Ilenia Perrella</maintainer>

  <license>Apache License 2.0</license>
  
  <buildtool_depend>ament_cmake_auto</buildtool_depend>

  <exec_depend>moveit_ros_move_group</exec_depend>
  <exec_depend>moveit_simple_controller_manager</exec_depend>
  <exec_depend>moveit_kinematics</exec_depend>
  <exec_depend>moveit_planners_ompl</exec_depend>
  <exec_depend>moveit_ros_visualization</exec_depend>
  <exec_depend>launch_pal</exec_depend>
  <exec_depend>moveit_ros_perception</exec_depend>
  <exec_depend>moveit_configs_utils</exec_depend>
  <exec_depend>talos_description</exec_depend>  


  <export>
      <build_type>ament_cmake</build_type>
  </export>

</package>
